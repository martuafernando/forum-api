{
  "name": "forum-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node src/app.js",
    "start:dev": "nodemon src/app.js",
    "test": "jest --setupFiles dotenv/config -i",
    "test:domain": "jest --setupFiles dotenv/config -i --testRegex \"src/Domains/.*\\.test\\.js$\"",
    "test:application": "jest --setupFiles dotenv/config -i --testRegex \"src/Applications/.*\\.test\\.js$\"",
    "test:infrastructure": "jest --setupFiles dotenv/config -i --testRegex \"src/Infrastructures/.*\\.test\\.js$\"",
    "test:interface": "jest --setupFiles dotenv/config -i --testRegex \"src/Interfaces/.*\\.test\\.js$\"",
    "test:watch:change": "jest --watch --setupFiles dotenv/config -i",
    "test:watch": "jest --watchAll --coverage --setupFiles dotenv/config -i",
    "migrate": "node-pg-migrate",
    "migrate:test": "node-pg-migrate -f config/database/test.json",
    "lint": "eslint ./src ./migrations ./tests --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/hapi": "^20.1.5",
    "@hapi/jwt": "^2.2.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "instances-container": "^2.0.3",
    "nanoid": "^3.1.23",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "eslint": "^8.45.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^27.0.6",
    "node-pg-migrate": "^5.10.0",
    "nodemon": "^2.0.9"
  }
}
